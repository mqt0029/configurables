[wsl2]

# An absolute Windows path to a custom Linux kernel.
# Default is the Microsoft built kernel provided inbox
# Eaxmple: C:\\temp\\myCustomKernel
kernel =

# How much memory to assign to the WSL 2 VM.
# Default to 50% of total memory on Windows or 8GB, whichever is less;
# on builds before 20175: 80% of your total memory on Windows
memory = 16GB

# How many processors to assign to the WSL 2 VM.
# Default is the same number of processors on Windows
processors = 6

localhostForwarding = true

# Additional kernel command line arguments.
# Example:vsyscall=emulate
kernelCommandLine =

# How much swap space to add to the WSL 2 VM, 0 for no swap file.
# Swap storage is disk-based RAM used when memory demand exceeds limit on hardware device.
# Default to 25% of memory size on Windows rounded up to the nearest GB
swap = 8GB

# An absolute Windows path to the swap virtual hard disk.
# Example: C:\\temp\\wsl-swap.vhdx
swapFile = %USERPROFILE%\\AppData\\Local\\Temp\\swap.vhdx

# Default true setting enables Windows to reclaim unused memory allocated to WSL 2 virtual machine.
pageReporting = true

# Boolean to turn on or off support for GUI applications (WSLg) in WSL.
# WARNING: Only available for Windows 11.
guiApplications = true

# Boolean to turn on an output console Window that shows the contents of dmesg upon start of a WSL 2 distro instance.
# WARNING: Only available for Windows 11.
debugConsole = false

# Boolean to turn on or off nested virtualization, enabling other nested VMs to run inside WSL 2.
# WARNING: Only available for Windows 11.
nestedVirtualization = true

# The number of milliseconds that a VM is idle, before it is shut down.
# WARNING: Only available for Windows 11.
vmIdleTimeout = 60000